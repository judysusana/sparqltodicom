<?xml version="1.0" encoding="UTF-8"?>
<consulta>
	<select>
		<variable>
		<etiqueta>?patientName
		</etiqueta> </variable>
		
		<variable>
		<etiqueta>?patientId
		</etiqueta> </variable>
		
		<variable>
		<etiqueta>?tag
		</etiqueta> </variable>
		
	</select>
		
	<where>
		<patron><tripla>?Patient, patientName, ?patientName	</tripla></patron>		
  		<patron><tripla>?Patient, patientRelatedStudy, ?Study</tripla></patron>
  		<patron><tripla>?Study, studyRelatedSerie, ?Serie</tripla></patron>
		<patron><tripla>?Serie, serieRelatedInstance, ?Image</tripla></patron>
		<patron><tripla>?Image, hasTag, ?Tag</tripla></patron>
		<patron><tripla>?Tag, tagLabel, ?Label</tripla></patron>
		
	</where>
	


<filter>
		<filtrovariable> <filtro>?Label</filtro>
  		<valorfiltro>cefalia</valorfiltro> </filtrovariable>
  		
	</filter>
	
